{
  "name": "@jupyterlab/latex",
  "version": "0.3.1",
  "description": "JupyterLab extension for running LaTeX",
  "author": "M Pacer & Ian Rose",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "schema/*.json",
    "style/*.*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "latex"
  ],
  "jupyterlab": {
    "extension": "lib/index.js",
    "schemaDir": "schema"
  },
  "scripts": {
    "build": "tsc",
    "build:test": "cd test && ./build-tests.sh",
    "test": "cd test && ./run-tests.sh",
    "clean": "rimraf lib",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.16.0",
    "@jupyterlab/apputils": "^0.16.2",
    "@jupyterlab/codeeditor": "^0.15.0",
    "@jupyterlab/coreutils": "^1.0",
    "@jupyterlab/docmanager": "^0.16.0",
    "@jupyterlab/docregistry": "^0.16.0",
    "@jupyterlab/fileeditor": "^0.16.0",
    "@jupyterlab/services": "^2.0",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/domutils": "^1.1.2",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.5.0",
    "pdfjs-dist": "2.0.104",
    "react": "~16.2.0",
    "react-dom": "~16.2.0"
  },
  "devDependencies": {
    "@types/pdfjs-dist": "^0.1.0",
    "@types/react": "~16.0.19",
    "@types/react-dom": "~16.0.5",
    "rimraf": "^2.5.2",
    "typescript": "~2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mpacer/jupyterlab-latex.git"
  },
  "resolutions": {
    "@types/react": "~16.0.19"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mpacer/jupyterlab-latex/issues"
  },
  "homepage": "https://github.com/mpacer/jupyterlab-latex"
}
